\begin_inset Formula 
\begin{eqnarray}
\mathbf{y}_{i,t}\mid\mathbf{z}_{i,t},\mathbf{D} & \sim & \text{multinomial}(1,\mathbf{D}\mathbf{z}_{it})\\
\mathbf{z}_{i,t}\mid\mathbf{z}_{i,t-1},\mathbf{S} & \sim & \text{multinomial}(1,\mathbf{S}\mathbf{z}_{i,t-1})\\
\mathbf{z}_{i,1}\mid\gamma_{1} & \sim & \text{\text{multinomial}}\left(1,(\gamma_{1},0,1-\gamma_{1})'\right)
\end{eqnarray}

\end_inset


