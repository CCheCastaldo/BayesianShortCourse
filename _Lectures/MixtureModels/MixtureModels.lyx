\begin_deeper
\begin_layout Standard

\lang american
\begin_inset Formula 
\begin{eqnarray*}
y_{i,t} & \sim & \text{Bernoulli}(z_{t}p_{t})\\
z_{i,t} & \sim & \text{Bernoulli}\left(z_{i,t-1}\phi_{t}+(1-z_{i,t-1})\gamma_{t}\right)\\
z_{1,t} & \sim & \text{Bernoulli}(\gamma_{1})
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard

\size small
\lang american
\begin_inset Formula 
\begin{flalign*}
[\phi,\bm{\gamma},\mathbf{p},\mathbf{Z}\mid\mathbf{Y}] & \propto\prod_{i=1}^{M+n_{z}}\prod_{t=2}^{T}\overbrace{[y_{i,t}|z_{i,t}p]_{t}}^{\text{observation}}\overbrace{[z_{i,t}|z_{i,t-1}\phi]}^{\text{survival}}+\overbrace{\prod_{k=1}^{t}(1-z_{i,k})\gamma_{t}]}^{\text{addition}}\\
 & \times\overbrace{[z_{1,t}\mid\gamma_{1}]}^{\text{initial conditions}}[\phi][\gamma_{t}][p_{t}].
\end{flalign*}

\end_inset


\end_layout

\begin_layout Itemize

\size footnotesize
\begin_inset Formula $n_{z}$
\end_inset

 = number of augmented data records
\end_layout

\begin_layout Itemize

\size footnotesize
\begin_inset Formula $\gamma_{i}$
\end_inset

 = probability of inclusion.
 Does not have biological interpretation.
\end_layout

\begin_layout Itemize

\size footnotesize
Approximate marginal posterior distribution of population size 
\begin_inset Formula $N$
\end_inset

 as a derived quantity 
\begin_inset Formula $N=\sum_{i=1}^{M+n_{z}}z_{i}$
\end_inset

.
\end_layout

\begin_layout Itemize

\size footnotesize
Does not require closure assumption.
\end_layout

