\begin_inset Formula 
\begin{eqnarray}
[\phi,\bm{\gamma},p,q,\mathbf{Z}\mid\mathbf{Y}] & \propto & \prod_{i=1}^{M}\prod_{t=2}^{T}[\mathbf{y}_{i,t}|\mathbf{D}\mathbf{z}_{i,t}][\mathbf{z}_{i,t}\mid\mathbf{S}\mathbf{z}_{i,t-1}][\mathbf{z}_{i,1}\mid\gamma_{1}]\\
 & \times & \left[\phi\right]\left[\gamma_{t}\right]\left[p\right]\left[q\right].
\end{eqnarray}

