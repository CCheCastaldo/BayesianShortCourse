\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\begin{align*}
y_{i}\sim & \begin{cases}
\text{Bernoulli}(\pi) & z_{i}=1\\
\text{\text{Bernoulli}(\rho)} & z_{i}=0
\end{cases}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
y_{i} & \sim & \text{Bernoulli}(z_{i}\phi)+\text{Bernoulli}\left((1-z_{i})\rho\right)\\
z_{i} & \sim & \text{Bernoulli}(\beta_{0}+\mathbf{x}_{i}\bm{\beta})\\{}
[\mathbf{\beta},\mathbf{z},\phi,\rho\mid\mathbf{y}] & \propto & \prod_{i=1}^{n}[y_{i}\mid z_{i}\phi+(1-z_{i})\rho]\\
 &  & \times[z_{i}\mid\beta_{0}+\mathbf{x}_{i}\bm{\beta}]\\
 &  & \times\text{priors}
\end{eqnarray*}

\end_inset

Will require informed priors on 
\begin_inset Formula $\phi$
\end_inset

 and 
\begin_inset Formula $\rho$
\end_inset

.
 They are not identifiable from test results alone.
\end_layout

\end_deeper
